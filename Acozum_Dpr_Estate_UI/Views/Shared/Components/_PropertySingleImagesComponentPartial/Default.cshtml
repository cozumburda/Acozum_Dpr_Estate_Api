@model List<GetProductImagesByProductIdDto>

@if (Model.Count>0)
{
    foreach (var item in Model)
    {
        <div class="item">
            <div class="card">
                <img src="@item.ImageUrl" class="img-fluid radius-image" alt="image">
            </div>
        </div>
    }
}
else
{
    <div class="item">
        <div class="card">
            <img src="~/starter/images/noimage.jpg" class="img-fluid radius-image" alt="image">
        </div>
    </div>
}

