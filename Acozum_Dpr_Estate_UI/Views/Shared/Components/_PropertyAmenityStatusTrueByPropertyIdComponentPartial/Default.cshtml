@model List<ResultPropertyAmenityByStatusTrueDto>
<div class="single-bg-white">
    <h3 class="post-content-title mb-4">Evin Diğer Özellikleri</h3>
    <ul class="details-list">
       @if (Model.Count>0)
        {
            foreach (var item in Model)
            {
                <li><strong>@item.Title </strong></li>
            }
        }
        else
        {
            <li><strong> Özellikler Tanımlanıyor.. </strong></li>
        }        
    </ul>
</div>